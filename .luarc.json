{
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": [
      "${3rd}/love2d/library"
    ],
    "ignoreDir": [
      "testing",
      "examples",
      ".git",
      "tasks",
      "themes",
      "profiling"
    ],
    "ignoreSubmodules": true
  },
  "diagnostics": {
    "globals": [
      "love"
    ]
  },
  "doc": {
    "privateName": [
      "_.*"
    ],
    "protectedName": []
  }
}
